<launch>

  <include file="$(find avt_341)/launch/base.launch">
    <arg name="waypoints_file" value="$(find avt_341)/config/waypoints.yaml" />
    <arg name="robot_description_file" value="$(find avt_341)/config/avt_bot.urdf" />
<!--     <arg name="grid_dilate" value="true" /> -->
  </include>

  <!-- Use this node to test the build if no simulator is being used. -->
  <node name="sim_test_node" pkg="avt_341" type="avt_341_sim_test_node" required="true" output="screen" />


</launch>